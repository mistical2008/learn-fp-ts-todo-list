        "lint": "pnpx eslint ./src",
        "lint:fix": "pnpx eslint ./src --fix",
        "compile": "tsc -p tsconfig.build.json",
        "build": "pnpm clean && pnpm compile && vite build",
        "build:nocheck": "pnpm clean && vite build",
        "preview": "pnpm build && vite preview",
